package com.biorecorder.basechart.grouping;


public class #E#GroupingAvg extends #E#GroupingFunction {
    private #e# sum;

    @Override
    public void reset() {
        super.reset();
        sum = 0;
    }

    @Override
    protected void add1(#e# value) {
        super.add1(value);
        sum += value;
    }

    @Override
    protected #e#[] groupedValue1() {
        #e#[] groupedValues = {sum / count};
        return groupedValues;
    }
}