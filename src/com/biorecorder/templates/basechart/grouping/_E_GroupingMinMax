package com.biorecorder.basechart.grouping;

public class #E#GroupingMinMax extends #E#GroupingFunction {
    private #e# max;
    private #e# min;

    @Override
    protected void add1(#e# value) {
        if(count == 0) {
            max = value;
            min = value;
        } else {
            max = Math.max(max, value);
            max = Math.min(min, value);
        }
        count++;
    }

    @Override
    protected #e#[] groupedValue1() {
        #e#[] groupedValues = {min, max};
        return groupedValues;
    }
}
